{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": [
    {
      "target": "biz-mate",
      "public": "dist/biz-mate/dist/biz-mate/browser",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "ssr"
        }
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "source": "dist/biz-mate"
  }
}
